# AWS Configuration for deployment
AWS_ACCOUNT_ID=123456789012  # Your 12-digit AWS account ID
AWS_REGION=us-east-2         # AWS region to deploy to

# Clerk Authentication (Frontend only needs the publishable key)
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key

# API configuration
VITE_BACKEND_URL=https://api.yourdomain.com

# Frontend Configuration
FRONTEND_PORT=5173

# Documenso integration
VITE_DOCUMENSO_PUBLIC_URL=https://docs.yourdomain.com

# Domain configuration
DOMAIN_NAME=yourdomain.com  # Used to construct default URLs

# Application Environment
VITE_ENV=production
ENV=production
DEBUG_MODE=false